{
	"name": "rightlines",
	"displayName": "RightLines",
	"description": "Remove duplicates, or lines matching selected text, and make such lines easily identifiable by marking them.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rightline.MarkThisLine",
		"onCommand:rightline.ClearGutterIcon",
		"onCommand:rightline.MarkArbitraryLine"
	],
	"main": "./out/rightline.js",
	"contributes": {
		"commands": [
			{
				"command": "rightline.MarkThisLine",
				"title": "Mark This Line",
				"category": "RightLines"
			},
			{
				"command": "rightline.ClearGutterIcon",
				"title": "Clear RightLines Marks",
				"category": "RightLines"
			},
			{
				"command": "rightline.MarkArbitraryLine",
				"title": "Mark A Specific Line...",
				"category": "RightLines"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}